<?xml version="1.0" encoding="utf-8" ?>
<Game Name="test" Width="800" Height="600" FirstScene="test" IsFullScreen="false" xmlns="http://www.kinectitude.com/2012/v1" X="15">
  
  <Prototype Name="prototype1" score="100">
    <Component Type="TransformComponent" X="1000" Y="2000"/>
    <Event Type="AttributeChanges">
      <Action Type="TransformComponent"/>
    </Event>
  </Prototype>

  <Prototype Name="prototype2" score="50" Prototype="prototype1">
    <Component Type="TransformComponent" X="5000" Y="5900"/>
    <Event Type="AttributeChanges">
      <Action Type="SetAction"/>
    </Event>
  </Prototype>

  <Prototype Name="prototype3" Prototype="prototype2">
    <Component Type="TransformComponent" Property3="10"/>
  </Prototype>

  <Prototype Name="prototype4" Prototype="prototype1 prototype2"/>

  <Scene Name="test" score="0" X="{game.X}">
    
    <Entity Name="myName" Prototype="prototype2 prototype3">
      <Component Type="TransformComponent" X="{scene.X}"/>

      <Event Type="SceneStartsEvent">
        <Action Type="CreateEntityAction" Prototype="prototype4"/>
      </Event>
    </Entity>

    <Entity Name="testEnum">
      <Component Type="TransformComponent" X="{myName.TransformComponent.Y}" Y="{myName.TransformComponent.Y}"/>
    </Entity>
  </Scene>
</Game>